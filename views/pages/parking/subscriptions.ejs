<div id="edit-subscriptions" v-cloak>

  <div class="container">
    <div class="row">
      <div class="col-md-5">{{monthName}}</div>
    </div>
    <div class="row">
      <div class="col-md-1">Mon</div>
      <div class="col-md-1">Tus</div>
      <div class="col-md-1">Wed</div>
      <div class="col-md-1">Thu</div>
      <div class="col-md-1">Fri</div>
    </div>

    <% dates.forEach( (date)=> { %>
      <div class="row">
        <div class="col-md-1">Mon</div>
        <div class="col-md-1">Tus</div>
        <div class="col-md-1">Wed</div>
        <div class="col-md-1">Thu</div>
        <div class="col-md-1">Fri</div>
      </div>
    <% } ); %>

  </div>
</div>
